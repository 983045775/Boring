<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.susion.boring">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:name=".SAppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splash.SplashActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainui.MainActivity" />
        <activity android:name=".music.activity.SearchMusicActivity" />
        <activity android:name=".music.activity.PlayMusicActivity" />
        <activity android:name=".music.activity.LocalMusicActivity" />
        <activity android:name=".music.activity.MyMusicCollectActivity"></activity>


        <service
            android:name=".music.service.MusicPlayerService"
            android:enabled="true"
            android:exported="false"
            />

    </application>



</manifest>